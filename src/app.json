{
  "expo": {
    "name": "Meetable",
    "slug": "meetable-app",
    "description": "Meetable Mobile Application",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/apple-icon.png",
    "scheme": "meetable",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/login-page.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.trevorflanigan.meetable-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
